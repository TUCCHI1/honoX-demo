{
  "compilerOptions": {
    "target": "ESNext", // コンパイル後のバージョン
    "module": "ESNext", // モジュールのバージョン 
    "moduleResolution": "Bundler", // モジュールの解決方法
    "strict": true, 
    "skipLibCheck": true, // d.tsファイルの型チェックをスキップ
    "lib": [
      "ESNext",
      "DOM"
    ], // コンパイル時に使用するライブラリ
    "types": [
      "vite/client",
      "@cloudflare/workers-types/2023-07-01"
    ],
    "jsx": "react-jsx", // JSXのバージョン https://www.typescriptlang.org/tsconfig#jsx
    "jsxImportSource": "react" // JSXのimport元
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}